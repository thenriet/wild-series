{% extends 'base.html.twig' %}

{% block body %}
	<h1>
		Trouve ta catégorie de séries !
	</h1>
	<div class="categories">
		{% for category in categories %}
			<div class="category">
				<a href="{{ path('category_show', {'categoryName': category.name}) }}">
					{{ category.name }}
				</a>
			</div>
		{% endfor %}
	</div>
{% endblock %}
